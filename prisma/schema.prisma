generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model Booking {
  id      Int    @id @default(autoincrement())
  name    String
  number  String
  email   String
  date    String
  time    String
  product String
}

model Products {
  id          Int     @id @default(autoincrement())
  icon        Boolean
  name        String
  price       Float
  description String
  productType String
}
